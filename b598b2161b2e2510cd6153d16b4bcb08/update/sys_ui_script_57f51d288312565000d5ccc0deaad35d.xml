    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Returns the R-IDE url, loading the specified app if specified.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_ride.GetRideUrl</name>
        <script><![CDATA[function GetRideUrl(strAppSysId)
{
	var strUrl = '/x_snc_ride_start.do';
	var strAppSysId = GetStringValue(strAppSysId);
	if (IsValidSysId(strAppSysId))
	{
		strUrl += '?app=' + strAppSysId;
	}
	return strUrl;
}]]></script>
        <script_name>GetRideUrl</script_name>
